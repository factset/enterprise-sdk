                            "city": "New York",
                            "country": "United States",
                            "postCode": "10001",
                            "state": "NY"
                          },
                          "socialMedia": {
                            "linkedin": "https://www.linkedin.com/company/partner-1",
                            "x": "https://x.com/partner-1",
                            "facebook": "https://www.facebook.com/partner-1"
                          }
                        }
                      ],
                      "meta": {
                        "pagination": {
                          "total": 1
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/401"
          },
          "403": {
            "$ref": "#/components/responses/403"
          },
          "429": {
            "$ref": "#/components/responses/429"
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "examples": {
                  "internalServerError": {
                    "$ref": "#/components/examples/internalServerError"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/partner/{id}": {
      "get": {
        "tags": [
          "Partners"
        ],
        "description": "Get a single FactSet Partner by ID.",
        "operationId": "getPartnerById",
        "parameters": [
          {
            "$ref": "#/components/parameters/id"
          }
        ],
        "responses": {
          "200": {
            "description": "Get a single FactSet Partner by ID.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PartnerSuccessResponse"
                },
                "examples": {
                  "partner": {
                    "value": {
                      "data": {
                        "id": "partner-1",
                        "partnerPageUrl": "https://www.factset.com/partner/catalog#/partner-details/partner-1",
                        "name": "Partner 1",
                        "logo": "https://www.example.com/logo.png",
                        "shortDescription": "Partner 1 short description",
                        "fullDescription": "<p>Partner 1 full description</p>",
                        "workflow": [
                          "Workflow 1",
                          "Workflow 2"
                        ],
                        "type": "technicalPartner",
                        "videoUrl": "https://www.example.com/video.mp4",
                        "companyUrl": "https://www.example.com",
                        "address": {
                          "addressLine1": "123 Main St.",
                          "addressLine2": "Suite 100",
                          "city": "New York",
                          "country": "United States",
                          "postCode": "10001",
                          "state": "NY"
                        },
                        "socialMedia": {
                          "linkedin": "https://www.linkedin.com/company/partner-1",
                          "x": "https://x.com/partner-1",
                          "facebook": "https://www.facebook.com/partner-1"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/401"
          },
          "403": {
            "$ref": "#/components/responses/403"
          },
          "404": {
            "description": "Partner not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "example": {
                  "errors": [
                    {
                      "id": "partner-not-found",
                      "title": "Partner not found",
                      "code": "404"
                    }
                  ]
                }
              }
            }
          },
          "429": {
            "$ref": "#/components/responses/429"
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "examples": {
                  "internalServerError": {
                    "$ref": "#/components/examples/internalServerError"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "parameters": {
      "id": {
        "name": "id",
        "in": "path",
        "description": "ID of the product or partner",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "_paginationLimit": {
        "name": "_paginationLimit",
        "in": "query",
        "description": "Limit for number of records returned per page",
        "required": false,
        "schema": {
          "type": "integer",
          "minimum": 1,
          "maximum": 100,
          "default": 10
        }
      },
      "_paginationOffset": {
        "name": "_paginationOffset",
        "in": "query",
        "description": "Specifies the starting point for pagination. This parameter is used to identify the beginning of next set of results.",
        "required": false,
        "schema": {
          "type": "integer",
          "default": 0,
          "minimum": 0,
          "maximum": 100
        }
      }
    },
    "schemas": {
      "ProductsSuccessResponse": {
        "description": "A success response for getting all products",
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "description": "An array of products",
            "items": {
              "$ref": "#/components/schemas/Product"
            }
          },
          "meta": {
            "$ref": "#/components/schemas/Meta"
          }
        },
        "required": [
          "data",
          "meta"
        ]
      },
      "ProductSuccessResponse": {
        "description": "A success response for getting a product",
        "type": "object",
        "properties": {
          "data": {
            "$ref": "#/components/schemas/Product"
          }
        },
        "required": [
          "data"
        ]
      },
      "PartnersSuccessResponse": {
        "description": "A success response for getting all partners",
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "description": "An array of partners",
            "items": {
              "$ref": "#/components/schemas/Partner"
            }
          },
          "meta": {
            "$ref": "#/components/schemas/Meta"
          }
        },
        "required": [
          "data"
        ]
      },
      "PartnerSuccessResponse": {
        "description": "A success response for getting a partner",
        "type": "object",
        "properties": {
          "data": {
            "$ref": "#/components/schemas/Partner"
          }
        },
        "required": [
          "data"
        ]
      },
      "AttributesGroupsItem": {
        "type": "object",
        "description": "Attribute group object",
        "properties": {
          "key": {
            "type": "string",
            "description": "Attribute group key, corresponding to a category of attributes. For example, 'Region' or 'History'.",
            "example": "Region"
          },
          "value": {
            "type": "string",
            "description": "Attribute group value, corresponding to a specific attribute within the category. For example, 'North America' or '10 + years'.",
            "example": "North America"
          },
          "isPreviewTag": {
            "type": "boolean",
            "description": "Boolean flag indicating whether the attribute's value is displayed on the product's card within the Open:FactSet Marketplace.",
            "example": false
          }
        }
      },
      "DocumentsItems": {
        "type": "object",
        "description": "Product documentation object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Document name",
            "example": "FactSet Product Documentation"
          },
          "section": {
            "type": "string",
            "description": "String detailing which section of the product page this document should be displayed in, i.e. 'Documentation' or 'Sample Data'.",
            "example": "Documentation"
          },
          "isPublic": {
            "type": "boolean",
            "description": "Boolean flag indicating whether the document is publicly accessible without authentication.",
            "example": true
          },
          "isFile": {
            "type": "boolean",
            "description": "Boolean flag indicating whether the document is a file for download or a URL.",
            "example": true
          },
          "url": {
            "type": "string",
            "format": "url",
            "description": "Document URL",
            "example": "https://www.example.com/factset-product-documentation.pdf"
          },
          "fileName": {
            "type": "string",
            "description": "Document file name, for file downloads.",
            "example": "factset-product-documentation.pdf"
          }
        }
      },
      "Partner": {
        "type": "object",
        "description": "Partner object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Partner unique identifier, used in the request URL",
            "example": "7GmIW2xGbdfZowcjuaMCmY"
          },
          "partnerPageUrl": {
            "type": "string",
            "format": "url",
            "description": "URL of the partner's page on the partner catalog.",
            "example": "https://www.factset.com/partner/catalog#/partner-details/partner-1"
          },
          "name": {
            "type": "string",
            "description": "Partner name",
            "example": "Partner 1"
          },
          "logo": {
            "type": "string",
            "format": "url",
            "description": "Partner logo URL",
            "example": "https://www.example.com/logo.png"
          },
          "shortDescription": {
            "type": "string",
            "description": "Partner short description (plain text), appearing on the partner's card within the FactSet Partner Catalog.",
            "example": "Partner 1 short description"
          },
          "fullDescription": {
            "type": "string",
            "description": "Partner full description (HTML)",
            "example": "<p>Partner 1 full description</p>"
          },
          "workflow": {
            "type": "array",
            "description": "An array of strings describing the Partner's workflow(s), used as categories on FactSet's Partner Catalog.",
            "items": {
              "type": "string"
            },
            "example": [
              "Workflow 1",
              "Workflow 2"
            ]
          },
          "type": {
            "type": "string",
            "enum": [
              "capabilityPartner",
              "consultant",
              "openFactSetPartner",
              "dataPartner",
              "technicalPartner"
            ],
            "description": "Partner type, describing the Partner's relationship with FactSet",
            "example": "technicalPartner"
          },
          "videoUrl": {
            "type": "string",
            "format": "url",
            "description": "Partner video URL",
            "example": "https://www.example.com/video.mp4"
          },
          "socialMedia": {
            "type": "object",
            "properties": {
              "linkedin": {
                "type": "string",
                "format": "url",
                "description": "Partner LinkedIn URL",
                "example": "https://www.linkedin.com/company/factset"
              },
              "x": {
                "type": "string",
                "format": "url",
                "description": "Partner X URL",
                "example": "https://x.com/factset"
              },
              "facebook": {
                "type": "string",
                "format": "url",
                "description": "Partner Facebook URL",
                "example": "https://www.facebook.com/factset"
              }
            },
            "description": "Partner social media URLs",
            "example": {
              "linkedin": "https://www.linkedin.com/company/factset",
              "twitter": "https://x.com/factset",
              "facebook": "https://www.facebook.com/factset"
            }
          },
          "companyUrl": {
            "type": "string",
            "format": "url",
            "description": "Partner company URL",
            "example": "https://www.example.com"
          },
          "address": {
            "type": "object",
            "properties": {
              "addressLine1": {
                "type": "string",
                "description": "Partner address line 1",
                "example": "123 Main St."
              },
              "addressLine2": {
                "type": "string",
                "description": "Partner address line 2, if applicable",
                "example": "Suite 100"
              },
              "city": {
                "type": "string",
                "description": "Partner city",
                "example": "New York"
              },
              "country": {
                "type": "string",
                "description": "Partner country",
                "example": "United States"
              },
              "postCode": {
                "type": "string",
                "description": "Partner post code",
                "example": "10001"
              },
              "state": {
                "type": "string",
                "description": "Partner state, if applicable",
                "example": "NY"
              }
            },
            "description": "Partner address",
            "example": {
              "addressLine1": "123 Main St.",
              "addressLine2": "Suite 100",
              "city": "New York",
              "country": "United States",
              "postCode": "10001",
              "state": "NY"
            }
          }
        },
        "required": [
          "id",
          "partnerPageUrl",
          "name",
          "logo",
          "shortDescription",
          "fullDescription",
          "workflow",
          "type"
        ]
      },
      "Product": {
        "type": "object",
        "description": "Product object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Product unique identifier",
            "example": "ahkl7OjFcr5Ud1dUNSFxW"
          },
          "productPageUrl": {
            "type": "string",
            "format": "url",
            "description": "URL of the product's page on the marketplace catalog.",
            "example": "https://www.factset.com/marketplace/catalog/product/product-title"
          },
          "secondaryLink": {
            "type": "string",
            "format": "url",
            "description": "Secondary link with additional information",
            "example": "https://www.example.com"
          },
          "title": {
            "type": "string",
            "description": "Product title",
            "example": "FactSet Product Title"
          },
          "description": {
            "type": "string",
            "description": "Product description",
            "example": "Short product description that appears on the product's card within the Open:FactSet Marketplace."
          },
          "createDate": {
            "type": "string",
            "format": "date",
            "description": "Product creation date",
            "example": "2021-09-01"
          },
          "partner": {
            "type": "object",
            "description": "Product partner object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Partner unique identifier, used in the URL"
              },
              "partnerPageUrl": {
                "type": "string",
                "format": "url",
                "description": "URL of the partner's page on the partner catalog."
              },
              "name": {
                "type": "string",
                "description": "Partner name"
              }
            },
            "example": {
              "id": "partner-1",
              "partnerPageUrl": "https://www.factset.com/partner/catalog#/partner-details/partner-1",
              "name": "Partner 1"
            }
          },
          "productStatus": {
            "type": "string",
            "enum": [
              "available",
              "candidate",
              "comingSoon",
              "developmentQueue",
              "new",
              "updated"
            ],
            "description": "Product status pertaining to its availability on the Open:FactSet Marketplace",
            "example": "available"
          },
          "type": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": [
                "dataFeed",
                "api",
                "platform"
              ]
            },
            "description": "Product type",
            "example": [
              "dataFeed"
            ]
          },
          "documents": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DocumentsItems"
            },
            "description": "Product documents",
            "example": [
              {
                "name": "FactSet Product Documentation",
                "section": "Documentation",
                "isPublic": true,
                "isFile": true,
                "url": "https://www.example.com/factset-product-documentation.pdf",
                "fileName": "factset-product-documentation.pdf"
              }
            ]
          },
          "thirdPartyUrls": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ThirdPartyUrls"
            },
            "description": "Product third party URLs",
            "example": [
              {
                "label": "Third Party URL Label",
                "link": "https://www.example.com"
              }
            ]
          },
          "primaryLink": {
            "type": "string",
            "format": "url",
            "description": "Link to primary product information, which is separate from the product's product page URL.",
            "example": "https://www.example.com"
          },
          "previewLink": {
            "type": "string",
            "format": "url",
            "description": "Link to additional information on the product, which is separate from the product's product page URL.",
            "example": "https://www.example.com"
          },
          "relatedProducts": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/RelatedProducts"
            },
            "description": "Related products",
            "example": [
              {
                "id": "product-2",
                "productPageUrl": "https://www.factset.com/marketplace/catalog/product/product-2",
                "title": "Product 2"
              }
            ]
          },
          "coverageTable": {
            "type": "string",
            "description": "Product coverage table (HTML)",
