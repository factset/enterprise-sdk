      "name": "Apache License, Version 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0"
    },
    "description": "This API provides read-only access to marketplace products and partners.",
    "title": "Open:FactSet Marketplace API"
  },
  "servers": [
    {
      "url": "https://api.factset.com/ofs/v0",
      "description": "Production server"
    }
  ],
  "tags": [
    {
      "name": "Products",
      "description": "Open:FactSet Marketplace Products"
    },
    {
      "name": "Partners",
      "description": "Open:FactSet Marketplace Partners"
    }
  ],
  "security": [
    {
      "FactSetApiKey": []
    },
    {
      "FactSetOAuth2": []
    }
  ],
  "externalDocs": {
    "description": "API Documentation",
    "url": "https://developer.factset.com/api-catalog/openfactset-marketplace-api"
  },
  "paths": {
    "/products": {
      "get": {
        "tags": [
          "Products"
        ],
        "description": "Get all products published on the Open:FactSet Marketplace Catalog.",
        "operationId": "getProducts",
        "parameters": [
          {
            "$ref": "#/components/parameters/_paginationLimit"
          },
          {
            "$ref": "#/components/parameters/_paginationOffset"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response to get all products published on the Open:FactSet Marketplace Catalog.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProductsSuccessResponse"
                },
                "examples": {
                  "products": {
                    "value": {
                      "data": [
                        {
                          "id": "product-1",
                          "productPageUrl": "https://www.factset.com/marketplace/catalog/product/product-1",
                          "title": "Product 1",
                          "description": "Product 1 description",
                          "createDate": "2021-09-01",
                          "partner": {
                            "id": "partner-1",
                            "partnerPageUrl": "https://www.factset.com/partner/catalog#/partner-details/partner-1",
                            "name": "Partner 1"
                          },
                          "relatedProducts": [
                            {
                              "id": "product-2",
                              "productPageUrl": "https://www.factset.com/marketplace/catalog/product/product-2",
                              "title": "Product 2"
                            },
                            {
                              "id": "product-3",
                              "productPageUrl": "https://www.factset.com/marketplace/catalog/product/product-3",
                              "title": "Product 3"
                            }
                          ],
                          "productStatus": "available",
                          "type": [
                            "dataFeed"
                          ]
                        }
                      ],
                      "meta": {
                        "pagination": {
                          "total": 1
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/401"
          },
          "403": {
            "$ref": "#/components/responses/403"
          },
          "429": {
            "$ref": "#/components/responses/429"
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "examples": {
                  "internalServerError": {
                    "$ref": "#/components/examples/internalServerError"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/product/{id}": {
      "get": {
        "tags": [
          "Products"
        ],
        "description": "Get a single Open:FactSet Marketplace Catalog product by ID.",
        "operationId": "getProductById",
        "parameters": [
          {
            "$ref": "#/components/parameters/id"
          }
        ],
        "responses": {
          "200": {
            "description": "Get a single Open:FactSet Marketplace Catalog product by ID.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProductSuccessResponse"
                },
                "examples": {
                  "product": {
                    "value": {
                      "data": {
                        "id": "product-1",
                        "productPageUrl": "https://www.factset.com/marketplace/catalog/product/product-1",
                        "title": "Product 1",
                        "description": "Product 1 description",
                        "createDate": "2021-09-01",
                        "partner": {
                          "id": "partner-1",
                          "partnerPageUrl": "https://www.factset.com/partner/catalog#/partner-details/partner-1",
                          "name": "Partner 1"
                        },
                        "relatedProducts": [
                          {
                            "id": "product-2",
                            "productPageUrl": "https://www.factset.com/marketplace/catalog/product/product-2",
                            "title": "Product 2"
                          },
                          {
                            "id": "product-3",
                            "productPageUrl": "https://www.factset.com/marketplace/catalog/product/product-3",
                            "title": "Product 3"
                          }
                        ],
                        "productStatus": "available",
                        "type": [
                          "dataFeed"
                        ]
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/401"
          },
          "403": {
            "$ref": "#/components/responses/403"
          },
          "404": {
            "description": "Product not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "example": {
                  "errors": [
                    {
                      "id": "product-not-found",
                      "title": "Product not found",
                      "code": "404"
                    }
                  ]
                }
              }
            }
          },
          "429": {
            "$ref": "#/components/responses/429"
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "examples": {
                  "internalServerError": {
                    "$ref": "#/components/examples/internalServerError"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/partners": {
      "get": {
        "tags": [
          "Partners"
        ],
        "description": "Get all partners published on FactSet's Partner Catalog.",
        "operationId": "getPartners",
        "parameters": [
          {
            "$ref": "#/components/parameters/_paginationLimit"
          },
          {
            "$ref": "#/components/parameters/_paginationOffset"
          }
        ],
        "responses": {
          "200": {
            "description": "Get all partners published on FactSet's Partner Catalog.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PartnersSuccessResponse"
                },
                "examples": {
                  "partners": {
                    "value": {
                      "data": [
                        {
                          "id": "partner-1",
                          "partnerPageUrl": "https://www.factset.com/partner/catalog#/partner-details/partner-1",
                          "name": "Partner 1",
                          "logo": "https://www.example.com/logo.png",
                          "shortDescription": "Partner 1 short description",
                          "fullDescription": "<p>Partner 1 full description</p>",
                          "workflow": [
                            "Workflow 1",
                            "Workflow 2"
                          ],
                          "type": "technicalPartner",
                          "videoUrl": "https://www.example.com/video.mp4",
                          "companyUrl": "https://www.example.com",
                          "address": {
                            "addressLine1": "123 Main St.",
