  "paths": {
    "/company/stock-movement-summary": {
      "get": {
        "tags": [
          "Stock Movement Summary"
        ],
        "summary": "Get Stock Movement Summary",
        "description": "Returns the stock price movement over the last trading period or intraday, and aims to explain that movement using StreetAccount news over the last two trading days.",
        "operationId": "getStockMovementSummary",
        "parameters": [
          {
            "name": "identifier",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Identifier",
              "description": "Ticker or FactSet Entity ID of the company"
            },
            "description": "Ticker or FactSet Entity ID of the company"
          },
          {
            "name": "outputType",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "title": "Output Type",
              "description": "Type of output to return. default for full summary and oneline for one-line summary.",
              "enum": [
                "default",
                "oneline"
              ],
              "default": "default"
            },
            "description": "Type of output to return. default for full summary and oneline for one-line summary."
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with stock movement data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StockMovementResponse"
                },
                "example": {
                  "data": [
                    {
                      "title": "Stock Movement Summary for Apple Inc.",
                      "content": "Apple Inc. experienced a significant stock price increase of 5.2% on April 15, 2025.",
                      "generatedTimestamp": "2025-04-15T12:00:00Z",
                      "references": [
                        {
                          "phrase": "significant stock price increase",
                          "url": "https://example.com/source1"
                        },
                        {
                          "phrase": "analyst upgrades",
                          "url": "https://example.com/source2"
                        }
                      ],
                      "competitors": [
                        {
                          "entityName": "Microsoft Corporation",
                          "entityId": "MSFT-US",
                          "priceChangePercentage": -0.29
                        }
                      ]
                    }
                  ],
                  "meta": {
                    "ticker": "AAPL-US",
                    "outputType": "default"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {
              "application/json": {
                "examples": {
                  "missing_identifier": {
                    "summary": "Missing Identifier",
                    "value": {
                      "errors": [
                        {
                          "id": "4365ABCD-1234-5678-90EF-1234567890AB",
                          "title": "Identifier must be a valid ticker or FactSet Entity ID. No Identifier was provided.",
                          "code": "MISSING_REQUIRED_FIELD"
                        }
                      ]
                    }
                  }
                },
                "schema": {
                  "$ref": "#/components/schemas/CustomValidationError"
                }
              }
            }
          },
          "401": {
            "description": "Authentication Failed"
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "example": {
                  "errors": [
                    {
                      "id": "436902342-1234-5678-90EF-1234567890AB",
                      "title": "Service is currently unavailable. Please try again later.",
                      "code": "SERVER_ERROR"
                    }
                  ]
                },
                "schema": {
                  "$ref": "#/components/schemas/ServerError"
                }
              }
            }
          }
        }
      }
    },
